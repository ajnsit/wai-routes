name          : kitchen-example
version       : 0.8.1
cabal-version : >=1.16
build-type    : Simple
license       : PublicDomain
maintainer    : ajnsit@gmail.com
synopsis      : Kitchen sink example for wai-routes
description   : Kitchen sink example for wai-routes
category      : Web
author        : Anupam Jain
data-dir      : ""

executable kitchen-example
    build-depends    : base >= 4.7  && < 5
                     , wai-routes
                     , warp
                     , text
    main-is          : Main.hs
    buildable        : True
    default-language : Haskell2010
    ghc-options      : -Wall
    hs-source-dirs : src
